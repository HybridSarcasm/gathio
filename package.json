{
  "name": "gathio",
  "version": "2.0.0",
  "description": "Nicer event sharing",
  "scripts": {
    "start": "node dist/index.js",
    "dev:sass": "sass --watch --update src/client/scss:dist/client/css",
    "build:sass": "sass --no-source-map --style=compressed src/client/scss:dist/client/css",
    "dev:client": "npx parcel watch src/client/js/index.js --dist-dir=dist/client/js",
    "dev:server": "nodemon --watch ./src/server -e ts,hbs --exec ts-node ./src/server/index.ts",
    "build:client": "npx parcel build src/client/js/index.js --dist-dir=dist/client/js",
    "build:server": "tsc",
    "build": "npm-run-all --parallel build:sass build:client build:server",
    "dev": "npm-run-all --parallel dev:sass dev:client dev:server"
  },
  "repository": "https://github.com/lowercasename/gathio",
  "author": "Raphael Kabo <mail@raphaelkabo.com>",
  "license": "BSD-3-Clause",
  "private": false,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "@alpinejs/mask": "^3.10.2",
    "@kingshott/iodine": "^7.0.1",
    "@vuelidate/core": "^2.0.0-alpha.41",
    "@vuelidate/validators": "^2.0.0-alpha.29",
    "@vvo/tzdb": "^6.50.0",
    "alpinejs": "^3.10.2",
    "axios": "^0.27.2",
    "choices.js": "^10.1.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.7",
    "esbuild": "^0.14.38",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-ejs-layouts": "^2.5.1",
    "express-handlebars": "^6.0.5",
    "express-validator": "^6.14.0",
    "jimp": "^0.16.1",
    "luxon": "^2.3.2",
    "mongoose": "^6.3.1",
    "multer": "^1.4.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sanitize-html": "^2.7.0",
    "select2": "^4.1.0-rc.0",
    "tom-select": "^2.0.1",
    "toml": "^3.0.0",
    "typescript": "^4.6.4",
    "vue": "^3.2.33",
    "vue-select": "^4.0.0-beta.3",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "2.5.0",
    "@types/express": "^4.17.13",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/luxon": "^2.3.1",
    "@types/multer": "^1.4.7",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "esbuild-sass-plugin": "^2.2.6",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.5.0",
    "process": "^0.11.10",
    "sass": "^1.51.0",
    "ts-node": "^10.7.0",
    "vue-template-compiler": "^2.6.14"
  }
}
